{
  "name": "cypress-cucumber-example",
  "version": "1.0.0",
  "description": "Example of cypress with cucumber",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --spec cypress/integration/search_scrum_post.feature",
    "cypress:tags": "cypress run -e TAGS='@e2e'",
    "cypress:report": "cypress run --reporter mochawesome && open ./mochawesome-report/mochawesome.html",
    "simple_build": "cd ../cy-cucumber-for-beginners && npm install",
    "static_fix": "standard --fix",
    "clean:report": "rimraf mochawesome-report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Eloyvj/PrjCypressExample.git"
  },
  "keywords": [
    "cypress",
    "cucumber",
    "testing"
  ],
  "homepage": "https://github.com/Eloyvj/PrjCypressExample",
  "devDependencies": {
    "standard": "^12.0.1",
    "cypress": "^3.1.3",
    "cypress-cucumber-preprocessor": "^1.9.1",
    "mocha": "^5.2.0",
    "mochawesome": "^3.1.1",
    "rimraf": "^2.6.2"
  },
  "author": "Eloy Vitorio",
  "license": "MIT"
}
